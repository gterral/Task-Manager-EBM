{% extends "@FOSUser/fosUserLayout.html.twig" %}

{% trans_default_domain 'FOSUserBundle' %}

{% block fos_user_content %}
    
    <div class="brand">
        <h2 class="brand-text font-size-18">{{ 'registration.confirmed'|trans({'%username%': user.username}) }}</h2>
    </div>
    {% if targetUrl %}
    <p><a href="{{ targetUrl }}">{{ 'registration.back'|trans }}</a></p>
    {% endif %}
{% endblock fos_user_content %}
